<h1>Listing all Articles</h1>
<p>
  <%= link_to "Create new article", new_article_path%>
</p>

<table>
  <theader>
    <th>Title</th>
    <th>Description</th>
  </theader>
    <tbody>
      <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.description %></td>
        <td><%= link_to 'Show', article_path(article)%>
        <td><%= link_to 'Edit', edit_article_path(article)%>
        <td><%= link_to 'Delete', '#'%>
      </tr>
      <% end %>
    </tbody>
  </table>